#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void) {

	int A = 0, B = 0;
	int ans3, ans4, ans5, ans6;
	int tmp;

	scanf("%d", &A);
	scanf("%d", &B);

	ans3 = A * (B % 10);
	ans4 = A * ((B % 100) / 10);
	ans5 = A * (B / 100);

	printf("%d\n%d\n%d\n%d", ans3, ans4, ans5, ans3+ans4*10+ans5*100);

	return 0;
}
