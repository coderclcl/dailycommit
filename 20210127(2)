#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

// https://www.acmicpc.net/problem/11720

int main(void) {

	int num, sum = 0, longNumber;
	scanf("%d", &num);

	char longNum[101] = { 0 };
	scanf("%s", longNum);

	for (int i = 0; i < num; i++) {
		sum += (longNum[i] - 48);
	}

	printf("%d", sum);

	return 0;
}
