#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int makeStar(int i, int j, int N);

// https://www.acmicpc.net/problem/2447

int main(void) {

	int N;

	scanf("%d", &N);

	for (int i = 0; i < N; i++) {

		for (int j = 0; j < N; j++) {

			makeStar(i, j, N);

		}
		printf("\n");

	}

	return 0;
}


int makeStar(int i, int j, int N) {

	if ((i/N) % 3 == 1 && (j/N) % 3 == 1) {
		printf(" ");
	}
	else {

		if (N / 3 == 0) {
			printf("*");
		}
		else {
			makeStar(i, j, N / 3);
		}

	}

	return 0;
}
